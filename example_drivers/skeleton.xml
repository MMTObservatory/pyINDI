        Idle
    </defLight>
    <defLight name="Light 2">
        Idle
    </defLight>
    <defLight name="Light 3">
        Idle
    </defLight>
    <defLight name="Light 4">
        Idle
    </defLight>
    <defLight name="Light 5">
        Idle
    </defLight>
</defLightVector>
<defSwitchVector device="SkeletonDevice" name="Switch Property" group="Main Control" state="Idle" perm="rw" rule="OneOfMany" timeout="60" label="Switch Vector">
    <defSwitch name="1x">
On
    </defSwitch>
    <defSwitch name="2x">
Off
    </defSwitch>
    <defSwitch name="3x">
Off
    </defSwitch>
    <defSwitch name="4x">
Off
    </defSwitch>
    <defSwitch name="5x">
Off
    </defSwitch>

</defSwitchVector>

<defTextVector device="SkeletonDevice" name="Text Property" label="Text Vector" group="Main Control" perm='rw' state="Idle" timeout="0">
    <defText name="Text 1" label="Text 1">
        Text
    </defText>
</defTextVector>

<defBLOBVector device="SkeletonDevice" name="BLOB Test" group="Main Control" state="Idle" perm="rw" timeout="60">
    <defBLOB name="BLOB 1">
    </defBLOB>
</defBLOBVector>
</INDIDriver>
