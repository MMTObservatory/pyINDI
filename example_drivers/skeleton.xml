<INDIDriver>
<defSwitchVector device="SkeletonDevice" name="CONNECTION" label="Connection" group="Main Control" state="Idle" perm="rw" rule="OneOfMany" timeout="60">
    <defSwitch name="CONNECT" label="Connect">
Off
    </defSwitch>
    <defSwitch name="DISCONNECT" label="Disconnect">
On
    </defSwitch>
</defSwitchVector>
<defNumberVector device="SkeletonDevice" name="Number Property" label="Number Vector" group="Main Control" state="Idle" perm="rw" timeout="0">
    <defNumber name="NUMBER_1" label="First" format="%10.6m" min="0" max="60" step="1">
3
    </defNumber>
    <defNumber name="NUMBER_2" label="Second" format="%10.6m" min="0" max="60" step="1">
3
    </defNumber>
</defNumberVector>
<defLightVector device="SkeletonDevice" name="Light Property" label="Light Vector" group="Main Control" state="Idle" timeout="0">
